<?xml version="1.0" encoding="UTF-8"?><scenarios name="uniqueName" defaultGroup="group-SSP2-CIA2-CDref-CCSref-REref-BEVref-NUCref"       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"       xsi:noNamespaceSchemaLocation="scenarios-schema.xsd">   <iterator name="pop" values="2, 5"/>    <iterator name="dds" values="A2, constant"/>    <iterator name="cons_dem" values="ref,high"/>    <iterator name="ccs" values="ref,no"/>    <iterator name="re" values="ref,adv"/>    <iterator name="bev" values="ref,adv"/>    <iterator name="nuc_ret" values="ref,fast"/>    <scenarioGroup name="group-SSP{pop}-CI{dds}-CD{cons_dem}-CCS{ccs}-RE{re}-BEV{bev}-NUC{nuc_ret}" useGroupDir="0" iterator="pop,dds,cons_dem,ccs,re,bev,nuc_ret">   <scenario name="SSP{pop}-CI{dds}-CD{cons_dem}-CCS{ccs}-RE{re}-BEV{bev}-NUC{nuc_ret}" baseline="1">			<add name="SSP">../input/policy/scenarios/pop_gdp_{pop}.xml</add>				<replace name="dd_usa">../input/policy/scenarios/HDDCDD_{dds}.xml</replace>				<add name="trn_pe_zero">../input/policy/scenarios/trn_pe_zero.xml</add>		<add name="trn_dem">../input/policy/scenarios/td_{pop}_{cons_dem}.xml</add>		<add name="FLSP">../input/policy/scenarios/FLSP_{pop}_{cons_dem}.xml</add>				<if value1="{ccs}" value2="no">				<add name="noccs_st">../input/policy/scenarios/noccs_st.xml</add>				<add name="noccs_pt">../input/policy/scenarios/noccs_pt.xml</add>		</if>				<if value1="{re}" value2="adv">				<add name="re_adv">../input/policy/scenarios/adv_re_tech.xml</add>		</if>				<if value1="{bev}" value2="adv">				<replace name="bev_UCD">../input/policy/scenarios/bev_UCD_adv.xml</replace>								<insert name="bev_USA_adv" after="bev_USA">../input/policy/scenarios/bev_USA_adv.xml</insert>				<add name="BEV_LoBatt_compact">../input/policy/reference/BEV_LoBatt_compact_wLearning.xml</add>				<add name="BEV_LoBatt_large">../input/policy/reference/BEV_LoBatt_large_wLearning.xml</add>				<add name="BEV_LoBatt_lttruck_SUV">../input/policy/reference/BEV_LoBatt_lttruck_SUV_wLearning.xml</add>				<add name="BEV_LoBatt_midsize">../input/policy/reference/BEV_LoBatt_midsize_wLearning.xml</add>				<add name="BEV_LDV_SW">../input/policy/reference/BEV_SW_update-global.xml</add>		</if>		<if value1="{nuc_ret}" value2="fast">				<function name="multiply">tag="nuc_usa",xpath="//half-life",value=0.6</function>				<add name="nuc_bound">../input/policy/scenarios/nonewnuclear.xml</add>						</if><!-- Note that RPS is NOT in config file -->        </scenario>    </scenarioGroup></scenarios>