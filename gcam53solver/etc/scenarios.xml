<?xml version="1.0" encoding="UTF-8"?>
<scenarios name="uniqueName" defaultGroup="group"

       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

       xsi:noNamespaceSchemaLocation="scenarios-schema.xsd">
<!--
This is for anything to be run with the 5.3 solver exe
-->

   	<iterator name="cap" values="USwide, state"/>
    <iterator name="ROW" values="2020ramp, 2005linear"/>  
    <scenarioGroup name="group" useGroupDir="0" iterator="cap,ROW">
        <scenario name="pyref-ssp2-53solver" baseline="1">
					<add name="SSP">../input/policy/scenarios/pop_gdp_2.xml</add>
					<replace name="solver">../input/policy/reference/cal_broyden_config53_relax.xml</replace>
        </scenario>
        <scenario name="60b50-{cap}-nobio-ROW{ROW}" iterator="cap,ROW">
					<add name="60b50US">../input/policy/scenarios/CO2pol/60b50_{cap}.xml</add>
					<add name="cap-ROW">../input/policy/scenarios/CO2pol/ROW_{ROW}.xml</add>
					<delete name="bio_ceiling_constraint_state.xml"/>
					<delete name="bio_ceiling_constraint_ROW.xml"/>
					<delete name="bio_ceiling_mkt_global.xml"/>
					<delete name="regional_bio_mkt"/>
        </scenario>   
        <scenario name="60b50-{cap}-BT-ROW{ROW}" iterator="cap,ROW">
        	<add name="60b50US">../input/policy/scenarios/CO2pol/60b50_{cap}.xml</add>
					<add name="cap-ROW">../input/policy/scenarios/CO2pol/ROW_{ROW}.xml</add>
        </scenario>              
        <scenario name="60b50-{cap}-BT0.5-ROW{ROW}" iterator="cap,ROW">
        	<add name="60b50US">../input/policy/scenarios/CO2pol/60b50_{cap}.xml</add>
					<add name="cap-ROW">../input/policy/scenarios/CO2pol/ROW_{ROW}.xml</add>        	
        	<function name="multiply">tag="bio_ceiling_constraint_state.xml",xpath="//constraint",value=0.5</function>
        	<function name="multiply">tag="bio_ceiling_constraint_ROW.xml",xpath="//constraint",value=0.5</function>
        </scenario>      

        <!--scenario name="60b50-{cap}-4x2015-noROW">
        	<function name="replaceValue">tag="bio_ceiling_constraint_state.xml",xpath="//constraint",value=24.116373246</function>
        	<function name="replaceValue">tag="bio_ceiling_constraint_ROW.xml",xpath="//constraint",value=88.34603198</function>
        </scenario-->   
    </scenarioGroup>
</scenarios>
