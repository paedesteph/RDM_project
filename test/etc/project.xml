<?xml version="1.0" encoding="UTF-8"?><projects xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"          xsi:noNamespaceSchemaLocation="project-schema.xsd">  <project name="test">    <vars>      <var name="startYear">2015</var>      <var name="endYear">2050</var>      <var name="years" eval="1">{startYear}-{endYear}</var>    </vars>	<scenariosFile name="scenarios.xml"/>    <steps>      <step name="setup" runFor="baseline">        @setup -b {baseline} -g {scenarioGroup} -S {scenarioSubdir} -w {scenarioDir}        -p {endYear} -y {years}      </step>      <step name="gcam"  runFor="baseline">@gcam -s {baseline} -S {projectXmlDir} </step>      <step name="query" runFor="baseline">        @query -o {batchDir} -w {scenarioDir} -s {scenario} -q "{queryXmlFile}"      </step>      <step name="setup" runFor="policy">            @setup -b {baseline} -s {scenario} -g {scenarioGroup} -S {scenarioSubdir}            -w {scenarioDir} -p {endYear} -y {years}      </step>      <step name="gcam"  runFor="policy">@gcam -s {scenario} -S {projectXmlDir}</step>      <step name="query" runFor="policy">        @query -o {batchDir} -w {scenarioDir} -s {scenario} -q "{queryXmlFile}"      </step>     </steps>    <queries varName="queryXmlFile">      <query name="nonCO2_emissions_by_sector"/>      <query name="prices_by_sector"/>      <query name="costs_by_subsector"/>      <query name="outputs_by_subsector"/>      <query name="elec_gen_by_aggregated_gen_tech"/>      <!-- <query name="elec_gen_by_aggregated_gen_tech" states="withUSA"/> -->    </queries>  </project></projects>