<?xml version="1.0" encoding="UTF-8"?><scenarios name="uniqueName" defaultGroup="group-CCSref-REref"       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"       xsi:noNamespaceSchemaLocation="scenarios-schema.xsd">           <iterator name="ccs" values="ref,no"/>    <iterator name="re" values="ref,adv"/>    <scenarioGroup name="group-CCS{ccs}-RE{re}" useGroupDir="0" iterator="ccs,re">        <scenario name="CCS{ccs}-RE{re}" baseline="1">		<if value1="{ccs}" value2="no">				<add name="noccs_st">../input/policy/scenarios/noccs_st.xml</add>				<add name="noccs_pt">../input/policy/scenarios/noccs_pt.xml</add>		</if>		<if value1="{re}" value2="adv">				<add name="re_adv">../input/policy/scenarios/adv_re_tech.xml</add>		</if>			        </scenario> 	<scenario name="CCS{ccs}-RE{re}-RPS">		<add name="L2245.StubTechRESSecondaryOutput_RPS_USA">../input/policy/reference/L2245.StubTechRESSecondaryOutput_RPS_USA.xml</add>		<add name="L2245.StubTechAdjCoef_RPS_USA">../input/policy/reference/L2245.StubTechAdjCoef_RPS_USA.xml</add>		<add name="L2245.RESPolicy_RPS_USA">../input/policy/reference/L2245.RESPolicy_RPS_USA.xml</add>	</scenario>    </scenarioGroup></scenarios>