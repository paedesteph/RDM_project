<?xml version="1.0" encoding="UTF-8"?><scenarios name="uniqueName" defaultGroup="group-CCSref-REref-COALslow"       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"       xsi:noNamespaceSchemaLocation="scenarios-schema.xsd">    <iterator name="ccs" values="ref,no"/>    <iterator name="re" values="ref,adv"/>    <iterator name="coal_ret" values="slow,fast"/>    <scenarioGroup name="group-CCS{ccs}-RE{re}-COAL{coal_ret}" useGroupDir="0" iterator="ccs,re,coal_ret">        <scenario name="base-CCS{ccs}-RE{re}-COAL{coal_ret}" baseline="1">		<if value1="{ccs}" value2="no">				<add name="noccs_st">../input/policy/scenarios/noccs_st.xml</add>				<add name="noccs_pt">../input/policy/scenarios/noccs_pt.xml</add>		</if>		<if value1="{re}" value2="adv">				<add name="re_adv">../input/policy/scenarios/adv_re_tech.xml</add>		</if>		<if value1="{coal_ret}" value2="fast">				<add name="coal_ret_fast">../input/policy/reference/coal_calibration_profit-shutdown_full.xml</add>										</if>				        </scenario> 	<scenario name="base-CCS{ccs}-RE{re}-COAL{coal_ret}-RPS">		<add name="L2245.StubTechRESSecondaryOutput_RPS_USA">../input/policy/reference/L2245.StubTechRESSecondaryOutput_RPS_USA.xml</add>		<add name="L2245.StubTechAdjCoef_RPS_USA">../input/policy/reference/L2245.StubTechAdjCoef_RPS_USA.xml</add>		<add name="L2245.RESPolicy_RPS_USA">../input/policy/reference/L2245.RESPolicy_RPS_USA.xml</add>	</scenario>    </scenarioGroup></scenarios>